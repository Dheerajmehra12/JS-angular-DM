    @font-face {
        font-family: 'Marcellus';
        font-style: normal;
        font-weight: 400;
        src: url('https://cdn.cmcd1.com/magnet/assets/fonts/Marcellus-Regular.ttf');
        src: local('Marcellus'), url('https://cdn.cmcd1.com/magnet/assets/fonts/Marcellus-Regular.woff') format('woff');
        font-display: swap;
    }

    @font-face {
        font-family: 'Arial';
        font-style: normal;
        font-weight: 400;
        src: local('Arial'), url('https://cdn.cmcd1.com/magnet/assets/fonts/ARIAL.woff') format('woff');
        font-display: swap;
    }

    @font-face {
        font-family: 'Manrope';
        font-style: normal;
        font-weight: 400;
        src: url('https://cdn.cmcd1.com/magnet/assets/fonts/manrope-regular.ttf');
        src: local('Manrope'), url('https://cdn.cmcd1.com/magnet/assets/fonts/manrope-regular.woff') format('woff'), url('https://cdn.cmcd1.com/magnet/assets/fonts/manrope-regular.woff2') format('woff2');
        font-display: swap;
    }

    html {
        -webkit-box-sizing: border-box;
        -moz-box-sizing: border-box;
        box-sizing: border-box;
        font-size: 100%;
        font-family: 'Arial',sans-serif;
        line-height: 1.15;
    }

    *, *:before, *:after {
        -webkit-box-sizing: inherit;
        -moz-box-sizing: inherit;
        box-sizing: inherit;
    }

    body {
        margin: 0;
        padding: 0;
        font-size: 1rem;
        line-height: 1.5;
    }
